<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

    <HBox prefHeight="50.0" prefWidth="200.0" styleClass="title-bar" stylesheets="@../../css/game.css" BorderPane.alignment="CENTER">
        <children>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="175.0" spacing="10.0" HBox.hgrow="NEVER">
                <padding>
                    <Insets left="10.0" />
                </padding>
                <children>
                    <Circle fill="#8a0000" radius="3.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                    <Circle fill="#014d00" radius="3.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                    <Circle fill="#014d00" radius="3.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <children>
                    <Label prefHeight="36.0" styleClass="title" stylesheets="@../../css/game.css" text="iDebug">
                        <font>
                            <Font size="24.0" />
                        </font>
                    </Label>
                </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefWidth="240.0" spacing="10.0" HBox.hgrow="NEVER">
                <padding>
                    <Insets bottom="10.0" right="10.0" top="10.0" />
                </padding>
                <children>
                    <HBox styleClass="large-circle-button" stylesheets="@../../css/game.css">
                        <children>
                            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../widgets/green-profile.png" />
                                </image>
                            </ImageView>
                        </children>
                    </HBox>
                    <HBox styleClass="large-circle-button" stylesheets="@../../css/game.css">
                        <children>
                            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../widgets/green-settings.png" />
                                </image>
                            </ImageView>
                        </children>
                    </HBox>
                    <HBox alignment="CENTER" styleClass="large-circle-button" stylesheets="@../../css/game.css">
                        <children>
                            <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="@../../widgets/green-exit .png" />
                                </image>
                            </ImageView>
                        </children>
                    </HBox>
                </children>
                <HBox.margin>
                    <Insets />
                </HBox.margin>
            </HBox>
        </children>
        <BorderPane.margin>
            <Insets bottom="5.0" top="1.0" />
        </BorderPane.margin></HBox>