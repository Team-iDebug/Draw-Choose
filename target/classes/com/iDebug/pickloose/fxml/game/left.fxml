<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minWidth="165.0" prefWidth="165.0" styleClass="rank" stylesheets="@../../css/game.css" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iDebug.pickloose.fxcontroller.FXGamePlayerController">
    <BorderPane.margin>
        <Insets bottom="1.0" right="1.0" top="1.0" />
    </BorderPane.margin>
    <children>
        <VBox fx:id="FXPlayerContainer" spacing="2.0" stylesheets="@../../css/game.css" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" left="5.0" />
            </padding>
        </VBox>
        <HBox alignment="CENTER" prefHeight="30.0" prefWidth="175.0" spacing="5.0">
            <children>
                <Button fx:id="FXKickBtn" mnemonicParsing="false" prefWidth="150.0" styleClass="butt-on" stylesheets="@../../css/game.css" text="Vote Kick" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" />
    </padding>
</VBox>
